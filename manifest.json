{
  "name": "Demo Secure Settings App",
  "author": {
    "name": "Developer Advocacy at Zendesk",
    "email": "developer-support@zendesk.com",
    "url": "https://developer.zendesk.com/documentation/"
  },
  "defaultLocale": "en",
  "private": true,
  "domainWhitelist": ["httpbin.org"],
  "location": {
    "support": {
      "ticket_sidebar": {
        "url": "assets/iframe.html",
        "flexible": true
      }
    }
  },
  "parameters": [
    {
      "name": "token",
      "type": "text",
      "secure": true,
      "default": "12345"
    },
    {
      "name": "subdomain",
      "type": "text",
      "secure": true,
      "default": "xyz"
    }
  ],
  "version": "1.0",
  "frameworkVersion": "2.0"
}
